            "id": 0,
            "definition": {
                "type": "query_value",
                "requests": [
                    {
                        "q": "avg:mapreduce.job.running_map_attempts{*}",
                        "aggregator": "avg"
                    }
                ],
                "custom_links": [],
                "title": "Running Mappers",
                "title_size": "16",
                "title_align": "left",
                "time": {
                    "live_span": "1h"
